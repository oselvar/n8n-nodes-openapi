{
	"name": "@oselvar/n8n-nodes-openapi",
	"version": "0.1.0",
	"description": "n8n nodes generated from OpenAPI specifications",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Aslak Helles√∏y",
		"email": "aslak.hellesoy@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/oselvar/n8n-nodes-openapi.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
