{
	"name": "@oselvar/n8n-nodes-openapi",
	"version": "0.1.1",
	"description": "n8n nodes generated from OpenAPI specifications",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Aslak Helles√∏y",
		"email": "aslak.hellesoy@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oselvar/n8n-nodes-openapi.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"test": "vitest run",
		"test:watch": "vitest",
		"format": "prettier --write ."
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"credentials": [
			"dist/credentials/OpenApiCredentialsApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/OpenApi/OpenApi.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "^0.17.0",
		"@types/node": "24.10.1",
		"eslint": "^9.39.1",
		"prettier": "^3.7.4",
		"release-it": "^19.0.6",
		"typescript": "^5.9.3",
		"vitest": "4.0.15"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@scalar/openapi-parser": "0.23.3",
		"openapi-types": "12.1.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
